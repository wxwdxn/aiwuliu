//保存计划派单
function  addScheduleSheetDummy(){
	var plateNumber=$("#plate_number").val();
	//如果车牌号填写
	if (plateNumber.trim().length!=0){
		if (objPlateNumber.flag=="0"){
			$("#typeName").val("");
			$("#position").val("");
			$("#status").val("");
			$("#smallModalInfo").modal();
			$("#smallModalInfo").find(".titleInfo").html("车辆不存在！")
