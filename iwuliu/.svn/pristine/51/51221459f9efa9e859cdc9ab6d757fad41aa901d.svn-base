.vehicleMonitoringMap {
  position: relative;
  height: 100%;
  background: #fff;
}
.vehicleMonitoringMap .stateOftheVehicle {
  position: absolute;
  left: 120px;
  left: 8rem;
  top: 9px;
  top: 0.9rem;
  background: #fff;
  box-shadow: 0px 0px 7px #0e0e0e;
  z-index: 9999;
}
.vehicleMonitoringMap .stateOftheVehicle div {
  width: 90px;
  width: 8rem;
  vertical-align: middle;
  height: 100%;
  padding-bottom: 9px;
}
.vehicleMonitoringMap .stateOftheVehicle div span {
  display: inline-block;
  font-size: 15px;
  font-size: 1.5rem;
}
.vehicleMonitoringMap .stateOftheVehicle div span img {
  margin: 6px 0;
}
.vehicleMonitoringMap .stateOftheVehicle div .checkbox {
  cursor: pointer;
  vertical-align: bottom;
  font-size: 20px;
}
.vehicleMonitoringMap .stateOftheVehicle div .allVehicles {
  margin-bottom: 0;
  margin-top: 16px;
}
.vehicleMonitoringCarTree {
  height: 100%;
  background: #efe;
}
.vehicleMonitoringCarTree label {
  font-weight: 100;
  font-size: 15px;
  font-size: 1.5rem;
}
