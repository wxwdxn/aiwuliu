public class T_Data_Company_AccountInfo {
    private  String account_name;                  // 账户名称
    private String  account_type;                  // 账户类型
    private  String  account_dept;                  //  所属公司
    private String  bank_info;                     // 银行卡信息
    private  Double  account_money;                  // 当前余额(元)
    private  Double  account_income;                   // 收入
    private Double account_withdraw;               // 提现
    private Double  account_pay;                   // 支出

    public String getAccount_name() {
        return account_name;
    }

    public void setAccount_name(String account_name) {
        this.account_name = account_name;
    }

    public String getAccount_type() {
        return account_type;
    }

    public void setAccount_type(String account_type) {
        this.account_type = account_type;
    }

    public String getAccount_dept() {
        return account_dept;
    }

    public void setAccount_dept(String account_dept) {
        this.account_dept = account_dept;
    }

    public String getBank_info() {
        return bank_info;
    }

    public void setBank_info(String bank_info) {
        this.bank_info = bank_info;
    }

    public Double getAccount_money() {
        return account_money;
    }

    public void setAccount_money(Double account_money) {
        this.account_money = account_money;
    }

    public Double getAccount_income() {
        return account_income;
    }

    public void setAccount_income(Double account_income) {
        this.account_income = account_income;
    }

    public Double getAccount_withdraw() {
        return account_withdraw;
    }

    public void setAccount_withdraw(Double account_withdraw) {
        this.account_withdraw = account_withdraw;
    }

    public Double getAccount_pay() {
        return account_pay;
    }

    public void setAccount_pay(Double account_pay) {
        this.account_pay = account_pay;
    }
}

